{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-eus-tertulia4-joaco-dev"
		},
		"LS_ADBK_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_ADBK'"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_SQL_CC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_CC'"
		},
		"LS_SQL_DATA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_DATA'"
		},
		"LS_ADBK_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0719-153544-pfwbjypj"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlseustertulia4joacodev.dfs.core.windows.net/"
		},
		"LS_KEYVAULT_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-ModernaDWH-Prod.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-eus-tertulia4-joaco-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADBK_accessToken": {
						"value": "[parameters('LS_ADBK_accessToken')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_SQL_CC_connectionString": {
						"value": "[parameters('LS_SQL_CC_connectionString')]"
					},
					"LS_SQL_DATA_connectionString": {
						"value": "[parameters('LS_SQL_DATA_connectionString')]"
					},
					"LS_ADBK_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_ADBK_properties_typeProperties_existingClusterId')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"LS_KEYVAULT_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KEYVAULT_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}